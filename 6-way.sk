  bit pred_pass_thru_if = rel_op(mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3), 0);

  if (pred_if) {
   if (pred_if_if) {
    state_1 = mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
    state_2 = mux4(state_2, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
   } else {
    state_1 = mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
    state_2 = mux4(state_2, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
   }
  } else if (pred_else_if) {
   if (pred_else_if_if) {
    state_1 = mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
    state_2 = mux4(state_2, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
   } else {
    state_1 = mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
    state_2 = mux4(state_2, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
   }
  } else {
   if (pred_pass_thru_if) {
    state_1 = mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
    state_2 = mux4(state_2, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3);
   } else {

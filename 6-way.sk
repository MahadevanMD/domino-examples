StateResult atom_template(int state_1, int state_2, int pkt_1, int pkt_2, int pkt_3, int pkt_4, int pkt_5) {
  bit pred_if         = rel_op(mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3), 0);
  bit pred_else_if    = rel_op(mux4(state_1, pkt_1, pkt_2, ??) + mux4(??, pkt_1, pkt_2, pkt_3) - mux4(??, pkt_1, pkt_2, pkt_3), 0);

